.outter{
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999;
    background-color: rgba(0,0,0,0.8);
    font-size: 28rpx;

}
.inner_bg{
    background-color: #ffffff;
    border-radius: 10rpx;
    padding: 30rpx;
    width: 84%;
    position: absolute;
    left: 50%;
    right: 0;
    top: 50%;
    bottom: 0;
    transform: translate(-50%,-50%);
}